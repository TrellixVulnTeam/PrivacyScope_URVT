{
  "manifest_version": 2,
  "name": "PrivacyScope",
  "version": "1.0",
  "icons": {
    "48": "icons/shield-48.png",
    "96": "icons/shield-96.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/shield-32.png",
    "default_title": "PrivacyScope",
    "default_popup": "popup/main_page.html"
  }

}
